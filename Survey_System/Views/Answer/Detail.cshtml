@model IEnumerable<Survey_System.Models.AnswerLine>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    int number = 1;
}

<div class="card">
    
    <div class="card-body"><br />
    <h4 class="text-center">Anket Detay sayfası</h4>
        <div class="table-responsive">

            @foreach (var item in Model)
            {
                <span><b>@number) </b> @item.Question</span><br /><b>@item.Answer</b><br />
                number++;
            }

        </div>
    </div>

</div>
